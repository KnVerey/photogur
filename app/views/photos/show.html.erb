<h1><%= @photo.title %></h1>
<h3><%= @photo.artist %></h3>
<%= image_tag(@photo.url) %>
<br>
<%= link_to button_tag("Edit this photo"), edit_photo_path(@photo) %>
<%= link_to button_tag("Back to photos"), photos_path %>
<%= link_to button_tag("Delete"), @photo, confirm: "Are you sure you want to delete this picture?", method: :delete %>
