<div class="jumbotron">
	<h1><%= @photo.title %></h1>
	<h3><%= @photo.artist %></h3>
</div>

<div class="center">
	<%= image_tag(@photo.url) %>
	<br><br>
	<%= link_to button_tag("Edit this photo"), edit_photo_path(@photo) %>
	<%= link_to button_tag("Delete this photo"), @photo, confirm: "Are you sure you want to delete this picture?", method: :delete %>
	<%= link_to button_tag("Back to photos"), photos_path %>
</div><br><br>